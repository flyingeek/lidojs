    "@babel/register": "^7.14.5",
    "@jest/types": "^25.5.0",
    "babel-jest": "^25.5.1",
    "babel-loader": "^8.2.0",
    "browserslist": "^4.16.6",
    "eslint": "^7.29.0",
    "follow-redirects": "^1.14.1",
    "jest": "^25.5.4",
    "ngeohash": "^0.6.3",
    "papaparse": "^5.3.1",
    "regenerator-runtime": "^0.13.7",
